datasource db {
    provider = "postgresql"
    url      = "postgres://gitpod@localhost/dailychecks"
}

generator client {
    provider      = "prisma-client-js"
    binaryTargets = ["native"]
}

model DailyCheck {
    id Int @id @default(autoincrement())
    date DateTime
    weight Float
    water Float
    hourssleep Float
    sleepqt Float
    note String
}